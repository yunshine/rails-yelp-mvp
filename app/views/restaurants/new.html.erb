<div class="container">
  <h3>Create a New Restaurant</h3>
</div>
<div class="container my-3">
  <%= simple_form_for (@restaurant) do |f| %>
  	<%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
